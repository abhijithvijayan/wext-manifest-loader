{
	"name": "wext-manifest",
	"version": "1.0.0",
	"description": "Webextension Manifest Generator",
	"license": "MIT",
	"author": "abhijithvijayan",
	"repository": "https://github.com/abhijithvijayan/wext-manifest.git",
	"engines": {
		"node": ">=8"
	},
	"scripts": {
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"pack:list": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz"
	},
	"files": [
		"index.js",
		"vendors.json",
		"transformVendorKeys.js"
	],
	"keywords": [
		"webextension",
		"manifest",
		"wext",
		"chrome",
		"firefox",
		"edge",
		"opera"
	],
	"devDependencies": {
		"babel-eslint": "^10.0.3",
		"eslint": "^6.6.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-onepass": "^1.4.3",
		"eslint-config-prettier": "^6.5.0",
		"eslint-plugin-html": "^6.0.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-react": "^7.16.0",
		"eslint-plugin-react-hooks": "^2.2.0",
		"prettier": "^1.18.2"
	}
}
